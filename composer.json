{
    "name": "deozza/philarmony-api-tester-bundle",
    "description": "A bundle to test Symfony API",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Edenn Touitou",
            "email": "deozza@gmail.com"
        }
    ],
    "version": "3.0.0",
    "require": {
        "php": "^7.0",
        "symfony/phpunit-bridge": "4.2.*",
        "doctrine/doctrine-fixtures-bundle": "^3.1",
        "symfony/browser-kit": "4.2.*",
        "symfony/dotenv": "4.2.*"
    },
    "autoload": {
        "psr-4": {
            "Deozza\\PhilarmonyApiTesterBundle\\": "src/"
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Deozza\\PhilarmonyApiTesterBundle\\Tests\\": "tests/"
        }
    }
}
